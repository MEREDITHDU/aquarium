; -----------------------------------------------------------

.MICRO      "ATMEGA16"
.TOOLCHAIN  "GCC"
.TARGET     "testuart.hex"
.SOURCE     "testuart.c"

.TRACE              ; Activate micro trace

; ------------------------------------------------------------

.POWER VDD=5 VSS=0  ; Power nodes
.CLOCK 8meg         ; Micro clock
.STORE 250m         ; Trace (micro+signals) storage time

; ------------------------------------------------------------

X1 TTY(19200 8 0 0 1 2) PD0 PD1  ; Place an interactive TTY cell.
.PLOT V(PD0) V(PD1)

